{
"manifest_version": 2,
    "name": "Messenger Enhancement Suite",
    "description": "A chrome extension to add additional functionality to messenger.com.",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}